{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "metrics": {},
    "tracing": {
      "isEnabled": false
    },
    "shared": {},
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "app": {
    "cron": {
      "enabled": false,
      "expression": "*/30 * * * * *",
      "failurePenaltySeconds": 1
    },
    "shouldLockObjectStorage": false,
    "shouldCollectInfo": false
  },
  "osmdbt": {
    "changesDir": "/tmp",
    "runDir": "/tmp",
    "logDir": "/tmp/log",
    "getLogMaxChanges": 50000,
    "verbose": false
  },
  "osmium": {
    "verbose": false,
    "progress": false
  },
  "objectStorage": {
    "endpoint": "http://s3.example",
    "bucketName": "bucketName",
    "acl": "public-read",
    "credentials": {
      "accessKey": "accessKey",
      "secretKey": "secretKey"
    },
    "region": "us-east-1"
  },
  "arstotzka": {
    "enabled": false,
    "serviceId": "serviceId",
    "mediator": {
      "timeout": 10000,
      "enableRetryStrategy": false,
      "actiony": {
        "url": "http://actiony.com"
      },
      "locky": {
        "url": "http://locky.com"
      }
    }
  }
}
