{
  "name": "osmdbt-wrapper",
  "version": "1.0.2",
  "description": "a wrapper for osmdbt",
  "main": "./dist/index.js",
  "scripts": {
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "standard-version",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json && npm run assets:copy",
    "start": "npm run build && cd dist && node ./index.js",
    "assets:copy": "copyfiles -f ./config/* ./dist/config && copyfiles ./package.json dist",
    "clean": "rimraf dist"
  },
  "author": "MapColonies",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.282.0",
    "@map-colonies/js-logger": "^0.0.5",
    "@map-colonies/telemetry": "^4.1.0",
    "@opentelemetry/api": "^1.4.0",
    "@opentelemetry/semantic-conventions": "^1.9.1",
    "config": "^3.3.9",
    "execa": "^5.1.1",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@map-colonies/eslint-config": "^3.1.3",
    "@map-colonies/prettier-config": "^0.0.1",
    "@map-colonies/standard-version-update-helm-version": "^2.0.1",
    "@types/config": "^3.3.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.0.0",
    "commitlint": "^13.2.1",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.15.0",
    "husky": "^8.0.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "standard-version": "^9.3.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.9.5"
  }
}
